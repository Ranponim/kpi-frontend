{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ci": "playwright test ci-workflow.spec.ts",
    "test:e2e:stable": "playwright test stable-workflow.spec.ts",
    "test:e2e:comprehensive": "playwright test comprehensive-workflow.spec.ts",
    "test:e2e:auxiliary": "playwright test auxiliary-features.spec.ts",
    "test:e2e:all": "playwright test comprehensive-workflow.spec.ts auxiliary-features.spec.ts stable-workflow.spec.ts",
    "test:e2e:smoke": "playwright test ci-workflow.spec.ts --project=chromium",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "perf:baseline": "node scripts/simple-performance-baseline.js",
    "perf:baseline:full": "node scripts/performance-baseline.js",
    "perf:lighthouse": "lhci autorun",
    "perf:analyze": "npm run build && npm run perf:lighthouse",
    "analyze:bundle": "cross-env ANALYZE=true npm run build",
    "analyze:bundle:win": "cross-env ANALYZE=true npm run build",
    "build:analyze": "npm run analyze:bundle && echo '번들 분석 완료: dist/bundle-analysis.html 파일 확인'",
    "build:optimized": "vite build --mode production",
    "dev:analyze": "npm run analyze:bundle && npm run dev",
    "perf:compare": "npm run perf:baseline && echo '성능 측정 완료 - 이전 결과와 비교하세요'"
  },
